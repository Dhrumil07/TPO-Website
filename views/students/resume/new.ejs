<% layout('layouts/boilerplate')%>
    <div class='row'>
        <h1 class="text-center mb-3">Create Resume</h1>
        <div class="col-12 ">
            <form id="regForm" action="/students/resume" novalidate method="post" class="row g-3 validated-form">
                <div class="tab col-12">

                    <h5 class="d-flex justify-content-center text-muted">PERSONAL DETAILS</h5>
                    <div class="row">
                        <div class="col-6">
                            <div class="mb-3">
                                <label for="firstName" class="form-label">First Name</label>
                                <input type="text" class="form-control" id="firstName" placeholder="Enter first name"
                                    name="personal[firstName]">
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="mb-3">
                                <label for="lastName" class="form-label">Last Name</label>
                                <input type="text" class="form-control" id="lastName" placeholder="Enter last name"
                                    name="personal[lastName]">
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-6">
                            <div class="mb-3">
                                <label for="mobile" class="form-label">Mobile No.</label>
                                <input type="tel" class="form-control" id="mobile" placeholder="Enter mobile no"
                                    name="personal[mobile]">
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="mb-3">
                                <label for="email" class="form-label">Email</label>
                                <input type="email" class="form-control" id="email" placeholder="Enter Email"
                                    name="personal[email]">
                            </div>
                        </div>
                    </div>


                    <div class="row">
                        <div class="col-6">
                            <div class="mb-3">
                                <label for="github" class="form-label">Github Link</label>
                                <input type="url" class="form-control" id="github" placeholder="Enter github link"
                                    name="personal[github]">
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="mb-3">
                                <label for="linkedin" class="form-label">LinkedIn Link</label>
                                <input type="url" class="form-control" id="linkedin" placeholder="Enter LinkedIn link"
                                    name="personal[linkedin]">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="tab">
                    <h5 class="d-flex justify-content-center text-muted mb-3">DEGREE COLLEGE INFORMATION</h5>
                    <div class="row">
                        <div class="col-6">
                            <div class="mb-3">
                                <label for="degreeCollege" class="form-label">College Name</label>
                                <input type="text" class="form-control" id="degreeCollege"
                                    placeholder="Enter college name" name="degreeCollege[name]">
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="mb-3">
                                <label for="location" class="form-label">College Location</label>
                                <input type="text" class="form-control" id="location"
                                    placeholder="Enter college location" name="degreeCollege[location]">
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-6">
                            <div class="mb-3">
                                <label for="degree" class="form-label">Degree</label>
                                <input type="text" class="form-control" id="degree" placeholder="Enter degree"
                                    name="degreeCollege[degree]">
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="mb-3">
                                <label for="branch" class="form-label">Branch</label>
                                <input type="text" class="form-control" id="branch" placeholder="Enter Branch"
                                    name="degreeCollege[branch]">
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-6">
                            <div class="mb-3">
                                <label for="cgpa" class="form-label">Cgpa</label>
                                <input type="number" class="form-control" id="cgpa" placeholder="Enter Cgpa"
                                    name="degreeCollege[cgpa]">
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="mb-3">
                                <label for="year" class="form-label">Year</label>
                                <input type="text" class="form-control" id="year" placeholder="Enter year"
                                    name="degreeCollege[year]">
                            </div>
                        </div>
                    </div>



                    <h5 class="d-flex justify-content-center text-muted mb-3 mt-3">JUNIOR COLLEGE
                        INFORMATION</h5>
                    <div class="row">
                        <div class="col-6">
                            <div class=" mb-3">
                                <label for="juniorCollege" class="form-label">College Name</label>
                                <input type="text" class="form-control" id="juniorCollege"
                                    placeholder="Enter college name" name="juniorCollege[name]">
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="mb-3">
                                <label for="location" class="form-label">College Location</label>
                                <input type="text" class="form-control" id="location"
                                    placeholder="Enter college location" name="juniorCollege[location]">
                            </div>
                        </div>
                    </div>


                    <div class="row">
                        <div class="col-4">
                            <div class="mb-3">
                                <label for="board" class="form-label">Board</label>
                                <input type="text" class="form-control" id="board" placeholder="Enter board"
                                    name="juniorCollege[board]">
                            </div>
                        </div>
                        <div class="col-4">
                            <div class="mb-3">
                                <label for="percentage" class="form-label">Percentage</label>
                                <input type="number" class="form-control" id="percentage" placeholder="Enter Percentage"
                                    name="juniorCollege[percentage]">
                            </div>
                        </div>
                        <div class="col-4">
                            <div class="mb-3">
                                <label for="year" class="form-label">Year</label>
                                <input type="text" class="form-control" id="year" placeholder="Enter year"
                                    name="juniorCollege[year]">
                            </div>
                        </div>
                    </div>


                    <div class="row">
                        <div class="col-6">
                            <div class="mb-3">
                                <label for="jee" class="form-label">JEE Score</label>
                                <input type="number" class="form-control" id="jee" placeholder="Enter Jee Score"
                                    name="juniorCollege[jee]">
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="mb-3">
                                <label for="cet" class="form-label">MHT-CET</label>
                                <input type="number" class="form-control" id="cet" placeholder="Enter MHT-CET Score"
                                    name="juniorCollege[cet]">
                            </div>
                        </div>
                    </div>

                    <h5 class="d-flex justify-content-center text-muted mb-3 mt-3">SCHOOL INFORMATION</h5>

                    <div class="row">
                        <div class="col-6">
                            <div class="mb-3">
                                <label for="school" class="form-label">School Name</label>
                                <input type="text" class="form-control" id="school" placeholder="Enter school name"
                                    name="school[name]">
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="mb-3">
                                <label for="location" class="form-label">School Location</label>
                                <input type="text" class="form-control" id="location"
                                    placeholder="Enter School location" name="school[location]">
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-4">
                            <div class="mb-3">
                                <label for="board" class="form-label">Board</label>
                                <input type="text" class="form-control" id="board" placeholder="Enter board"
                                    name="school[board]">
                            </div>
                        </div>
                        <div class="col-4">

                            <div class="mb-3">
                                <label for="percentage" class="form-label">Percentage</label>
                                <input type="number" class="form-control" id="percentage" placeholder="Enter Percentage"
                                    name="school[percentage]">
                            </div>
                        </div>
                        <div class="col-4">
                            <div class="mb-3">
                                <label for="year" class="form-label">Year</label>
                                <input type="text" class="form-control" id="year" placeholder="Enter year"
                                    name="school[year]">
                            </div>
                        </div>
                    </div>
                </div>

                <div class="tab">
                    <div id="Skill">
                        <button type='button' onclick="addSkill()" id="AddSkill" class="mb-4 btn btn-primary">Add
                            Skill</button>
                        <div id="Skill1"></div>
                    </div>
                    <br>
                    <div id="Project">
                        <button type='button' onclick="addProject()" id="AddProject" class="mb-4 btn btn-primary">Add
                            Project</button>
                        <div id="Project1"></div>
                    </div>
                    <br>
                    <div id="Achive">
                        <button type='button' onclick="addAchive()" id="AddAchive" class="mb-4 btn btn-primary">Add
                            Achievements</button>
                        <div id="Achive1"></div>
                    </div>
                </div>

                <div style="overflow:auto;">
                    <div class="d-flex justify-content-center">
                        <div class="buttons mt-2">
                            <button class="blob-btn mt-2 me-2" type="button" id="prevBtn" onclick="nextPrev(-1)">
                                Previous
                                <span class="blob-btn__inner">
                                    <span class="blob-btn__blobs">
                                        <span class="blob-btn__blob"></span>
                                        <span class="blob-btn__blob"></span>
                                        <span class="blob-btn__blob"></span>
                                        <span class="blob-btn__blob"></span>
                                    </span>
                                </span>
                            </button>
                        </div>
                        <div class="buttons mt-2">
                            <button class="blob-btn mt-2" type="button" id="nextBtn" onclick="nextPrev(1)">
                                Next
                                <span class="blob-btn__inner">
                                    <span class="blob-btn__blobs">
                                        <span class="blob-btn__blob"></span>
                                        <span class="blob-btn__blob"></span>
                                        <span class="blob-btn__blob"></span>
                                        <span class="blob-btn__blob"></span>
                                    </span>
                                </span>
                            </button>
                        </div>
                    </div>
                </div>

                <div style="text-align:center;" class="mb-5">
                    <span class="step"></span>
                    <span class="step"></span>
                    <span class="step"></span>
                </div>

            </form>
        </div>
    </div>

    <script src="/javascripts/tabs.js"></script>
    <script src="/javascripts/dynamic.js"></script>